<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- Report theme -->
        <!-- STANDARD, DARK -->
        <theme>STANDARD</theme>

        <!-- Document encoding -->
        <encoding>UTF-8</encoding>

        <!-- Protocol for script and stylesheets -->
        <!-- HTTP, HTTPS -->
        <protocol>HTTPS</protocol>

        <!-- Title of the report -->
        <documentTitle>Myntra E2E Automation Test Report</documentTitle>

        <!-- Report name - displayed at top nav -->
        <reportName>Myntra Test Execution Report</reportName>

        <!-- Global date format override -->
        <timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>

        <!-- Custom JavaScript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {

                });
            ]]>
        </scripts>

        <!-- Custom CSS -->
        <styles>
            <![CDATA[

            ]]>
        </styles>
    </configuration>
</extentreports>